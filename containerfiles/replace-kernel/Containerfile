RUN --mount=type=bind,from=ghcr.io/ublue-os/coreos-testing-kernel:41,source=/tmp/rpms,target=/tmp/kernel-rpms \
    rpm-ostree cliwrap install-to-root / && \
    rpm-ostree override remove kernel kernel-core kernel-modules kernel-modules-core kernel-modules-extra \
    --install /tmp/kernel-rpms/kernel-[0-9]*.rpm \
    --install /tmp/kernel-rpms/kernel-core-*.rpm \
    --install /tmp/kernel-rpms/kernel-modules-*.rpm && \
    ostree container commit